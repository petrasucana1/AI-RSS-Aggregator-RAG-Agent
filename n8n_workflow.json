    {
      "parameters": {
        "promptType": "define",
        "text": "=Choose 5 most relevant AI-related articles:\nGive only the result in this format:\nTitle:{{ $json.title }}  \nAuthor:{{ $json.creator }}  \nLink:{{ $json.link }}  \nArticle text:  \n{{ $json.content }}",
