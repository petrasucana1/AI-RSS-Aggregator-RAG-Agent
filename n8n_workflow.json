      "type": "@n8n/n8n-nodes-langchain.lmChatOpenAi",
      "typeVersion": 1.2,
      "position": [
        1248,
        560
      ],
      "id": "9958ea1f-5ba1-4f64-aad9-9f2d039c22cb",
      "name": "OpenAI Chat Model",
      "credentials": {
        "openAiApi": {
          "id": "yzhvg68IdO822akv",
          "name": "OpenAi account"
        }
      }
    },
    {
      "parameters": {
        "jsCode": "let allItems = $input.all();\nlet parsedArticles = [];\n\nfor (let item of allItems) {\n    let text = item.json.text;\n    if (!text) continue;\n\n    text = text.trim();\n\n    let rawArticles = text.split(/\\n\\s*Title:/).map((t, i) => i === 0 ? t : \"Title:\" + t);\n\n    for (let raw of rawArticles) {\n        if (!raw.trim()) continue;\n\n        let titleMatch = raw.match(/Title:\\s*(.*?)(\\n|$)/);\n        let authorMatch = raw.match(/Author:\\s*(.*?)(\\n|$)/);\n        let linkMatch = raw.match(/Link:\\s*(.*?)(\\n|$)/);\n        let textMatch = raw.match(/Article text:\\s*([\\s\\S]*?)(?=$|\\nTitle:)/);\n\n        parsedArticles.push({\n            json: {\n                title: titleMatch ? titleMatch[1].trim() : \"\",\n                author: authorMatch ? authorMatch[1].trim() : \"\",\n                link: linkMatch ? linkMatch[1].trim() : \"\",\n                text: textMatch ? textMatch[1].trim() : \"\"\n            }\n        });\n    }\n}\n\nreturn parsedArticles;\n"
      },
